<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LinearLayoutMain">
        <item name="android:background">@color/black</item>
     </style>

    <style name="TextActivitySettings">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">@dimen/size_text_in_setting_activity</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextHeaderActivity">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:textFontWeight">400</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">text</item>
        <item name="android:background">@drawable/search_background_edit_view</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textColor">@color/black</item>
    </style>

</resources>